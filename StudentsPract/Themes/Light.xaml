<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- xmlns:local="clr-namespace:StudentsPract.Themes"> -->

    <SolidColorBrush x:Key="Foreground" Color="Black"/>
    <SolidColorBrush x:Key="GrayColor" Color="Black"/>
    <SolidColorBrush x:Key="TitleColor" Color="Black"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#33E9E9E9"/>
    <SolidColorBrush x:Key="BorderBrushHover" Color="#FF0078D7"/>
    <SolidColorBrush x:Key="SidebarColor" Color="#FF818181"/>
    <SolidColorBrush x:Key="Item.MouseOver.Background" Color="#2B2B2B"/>
    <SolidColorBrush x:Key="Item.MouseOver.BackgroundLight" Color="#383838"/>


    <Style x:Key="WindowStyle" TargetType="Window">
        <Setter Property="Background" Value="#FFE6E6E6" />
        <Setter Property="Foreground" Value="{DynamicResource Foreground}"/>
        <Setter Property="FontSize" Value="16"/>
    </Style>
</ResourceDictionary>